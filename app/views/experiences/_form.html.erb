<%= simple_form_for(@experience) do |f| %>
  <%= f.error_notification %>
<%= f.input :title %>
<%= f.input :description %>
<%= f.input :location %>
<%= f.input :availability, collection: Experience::AVABILITY %>
<%= f.association :category  %>
<%= f.input :photos, as: :attachinary %>
<%= f.submit %>
<% end %>
